{
  "manifest_version": 2,
  "name": "laf助手",
  "version": "1.0.2",
  "description": "通过chatGPT写laf云函数",
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "browser_action": {
    "default_icon": "logo.png",
    "default_title": "laf助手",
    "default_popup": "options.html"
  },
  "permissions": ["activeTab","storage"],
  "options_page": "options.html",
  "commands": {
    "addString": {
      "suggested_key": {
        "default": "Ctrl+B",
        "mac": "Command+B"
      },
      "description": "通过chatGPT写laf云函数"
    }
  }
}
